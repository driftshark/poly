{
	"name": "poly",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json",
		"**/LICENSE.txt",
		"**/ThirdPartyNoticeText.txt"
	],
	"tree": {
		"$className": "DataModel",
		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$className": "Folder",
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"$path": "node_modules/@rbxts",
					"@driftshark": {
						"$path": "node_modules/@driftshark"
					}
				}
			},
			"poly": { "$path": "out" }
		}
	}
}
